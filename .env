# Mullvad Configuration (Required)
MULLVAD_ACCOUNT=1234567890woof

# Application Selection (transmission or qbittorrent)
APP=transmission

# Mullvad Server Selection
MULLVAD_COUNTRY=us
MULLVAD_CITY=nyc
MULLVAD_PORT=51820

# Mullvad Multihop (uncomment both if needed)
# MULLVAD_MULTIHOP_ENTRY=us
# MULLVAD_MULTIHOP_EXIT=de

# Key Rotation (0 = never)
WG_KEY_RENEW_DAYS=0

# Transmission WebUI Authentication
