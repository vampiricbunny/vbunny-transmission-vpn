OPENVPN_PASSWORD=
OPENVPN_OPTS=

# UFW Extra Ports (comma-separated)
UFW_EXTRA_PORTS=

# Web Proxy
WEBPROXY_USERNAME=
WEBPROXY_PASSWORD=

# Health Check
HEALTH_CHECK_HOST=google.com

# Timezone
TZ=America/New_York

# Version
